{
  "compilerOptions": {

    "module": "es2020",
    "moduleResolution": "bundler",
    "types": [
      "vitest/globals",
    ],    
    "target": "es2020",
    "outDir": "dist",
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
  },
  "exclude": [
    "node_modules",
    "cdk.out",
    "dist"
  ]
}