{
  "name": "aws-ca",
  "version": "0.1.0",
  "bin": {
    "aws-ca": "bin/aws-ca.js"
  },
  "scripts": {
    "build:lambda": "node esbuild.config.js",
    "build:cdk": "tsc -p tsconfig.cdk.json",
    "build": "npm run build:lambda && npm run build:cdk",
    "watch": "tsc -w",
    "test": "vitest",
    "cdk": "cdk",
    "package": "npm run build:lambda && cdk synth",
    "deploy": "npm run build:lambda && cdk deploy",
    "destroy": "cdk destroy"
  },
  "devDependencies": {
    "@types/node": "22.16.5",
    "aws-cdk": "2.1022.0",
    "esbuild": "^0.25.8",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@aws-solutions-constructs/aws-apigateway-lambda": "^2.86.0",
    "@types/aws-lambda": "^8.10.152",
    "aws-cdk-lib": "2.207.0",
    "constructs": "^10.4.2",
    "vitest": "^3.2.4"
  }
}
